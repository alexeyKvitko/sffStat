<?xml version="1.0"?>
<set xmlns:android="http://schemas.android.com/apk/res/android" android:interpolator="@android:anim/bounce_interpolator">
    <translate
        android:duration="100"
        android:fromXDelta="0"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toXDelta="-10"/>
    <translate
        android:duration="200"
        android:fromXDelta="-10"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toXDelta="10"/>
    <translate
        android:duration="100"
        android:fromXDelta="10"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toXDelta="0"/>

</set>